//draw A
static public void drawa(){
		int j=0,i=0;
		for(i=0;i<=5;i++){
			for(j=0;j<5;j++){
				if(i==0 || i==3){
					System.out.print("*");
				}
				else{
					if(j==0){
						System.out.print("*");
					}
					if(j==4){
						System.out.print("\t*");
					}
				}
				System.out.print(" ");
			}
			System.out.println("");
		}
	}
